@page
@model Leads.LeadsListModel
@{
    ViewData["Title"] = "LeadsList";
}

<h2>LeadsList</h2>


<div class="list-group">
    
   @foreach (var item in Model.LeadList)
   {
    <a class="list-group-item list-group-item-action" asp-page="./Leads/Details" asp-route-Id="@item.Id">@item.Surname</a>
   }
   </div>